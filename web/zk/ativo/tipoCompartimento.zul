		<grid width="100%">
			<rows>
				<row spans="4">
					<groupbox width="100%" mold="3d" open="false">
						<caption label="Contagem de Partículas"/>
						<grid>
							<columns>
								<column hflex="1"/>
								<column hflex="2"/>
								<column hflex="1"/>
